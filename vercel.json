{
    "version": 2,
    "cleanUrls": true,
    "public": true,
    "builds": [
      { "src": "index.html", "use": "@vercel/static" },
      { "src": "image/**", "use": "@vercel/static" },
      { "src": "css/**", "use": "@vercel/static" },
      { "src": "js/**", "use": "@vercel/static" }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }